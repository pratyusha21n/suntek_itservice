#include<stdio.h>
#include<string.h>
int main()
{
    char s1[20], s2[20];
    int l, l1, l2, i, j, f=0, nf=0;
    printf("Enter s1: ");
    gets(s1);
    printf("Enter s2: ");
    gets(s2);
    l1 = strlen(s1);
    l2 = strlen(s2);
    if(l1 == l2)
    {
        l = l1;
        for(i=0; i<len; i++)
        {
            f = 0;
            for(j=0; j<len; j++)
            {
                if(s1[i] == s2[j])
                {
                    f = 1;
                    break;
                }
            }
            if(f == 0)
            {
                nf = 1;
                break;
            }
        }
        if(nf == 1)
            printf("\nStrings are not Anagram");
        else
            printf("\nStrings are Anagram");
    }
    else
        printf("\nBoth string must contain same number of character to be an Anagram Strings");
    getch();
    return 0;
}

